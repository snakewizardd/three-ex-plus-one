0:complex_brot(
0:xmin = -10,
0:xmax = 10,
0:nx = 360,
0:ymin = -10,
0:ymax = 10,
0:ny = 360,
0:n=1000,
0:showplot = TRUE)
0:#Sourcing the function to calculate the full permutation sets
0:source('./libraries.R')
0:source('./functions/three-ex-function.R')
0:setwd("~/Desktop/github/three-ex-plus-one")
0:#Sourcing the function to calculate the full permutation sets
0:source('./libraries.R')
0:source('./functions/three-ex-function.R')
0:k <- matrix(0,nrow=100,ncol=10)
0:length(k)
0:#Sourcing the function to calculate the full permutation sets
0:source('./libraries.R')
0:source('./functions/three-ex-function.R')
0:#Creating a random set of data
0:random_set <- 1:10000
0:#Populating a matrix with our z values
0:z <- matrix(random_set,nrow=100,ncol=100)
0:#initializing k matrix to store results
0:k <- matrix(0,nrow=100,ncol=100)
0:#Creating z/k index
0:index <- which(z[,] != 1)
0:#Empty set to contain full outputs
0:full_output <- NULL
0:#Performing operations on all members of z and storing max values in k
0:for(index_number in index){
0:k[index_number] <-  max(calc(z[index_number])$capture_iteration)
0:full_output <- rbind(full_output,calc(z[index_number]))
0:}
0:#Intialzing empty set to contain rows-wise output format from the matrix
0:rows <- NULL
0:for(row in 1:dim(z)[1]){
0:for(col in 1:dim(z)[2]){
0:rows <- rbind(rows,data.frame(z_value = z[row,col], k_value = k[row,col]))
0:}
0:}
0:#Arranging row-wise output
0:rows <- rows %>% unique() %>%
0:arrange(k_value)
0:#Analyzing row-output trends
0:trends <- rows %>% group_by(k_value) %>%
0:count() %>% arrange(-n)
0:#Full data for trends
0:trend_ids <- rows %>% filter(k_value %in% trends$k_value[1:10])
0:full_trends_data <- full_output %>% filter(original_number %in% trend_ids$z_value)
0:#Create directory for this experiment
0:Experiment_ID <- 'first-ten-thousand'
0:data_path <- paste0('./data_files/','experiment_',Experiment_ID)
0:image_path <- paste0('./image_files/','experiment_',Experiment_ID)
0:dir_create(data_path)
0:dir_create(image_path)
0:write_csv(full_output,paste0(data_path,'/full_output.csv'))
0:write_csv(rows,paste0(data_path,'/z_and_k_values.csv'))
0:write_csv(trends,paste0(data_path,'/trend_values.csv'))
0:write_csv(full_trends_data,paste0(data_path,'/top_10_trend_values_full.csv'))
0:write_csv(as.data.frame(z),paste0(data_path,'/z_matrix.csv'))
0:write_csv(as.data.frame(k),paste0(data_path,'/k_matrix.csv'))
0:source('./functions/plot_all_members.R')
0:png(paste0(image_path,"/top3.png"),width = 1000, height = 1000)
0:par(mfrow=c(1,3))
0:plot_all_members(as.numeric(trends$k_value[1]),rows)
0:plot_all_members(as.numeric(trends$k_value[2]),rows)
0:plot_all_members(as.numeric(trends$k_value[3]),rows)
0:dev.off()
0:tiff(paste0(image_path,"/top3.tiff"),width = 1000, height = 1000)
0:par(mfrow=c(1,3))
0:plot_all_members(as.numeric(trends$k_value[1]),rows)
0:plot_all_members(as.numeric(trends$k_value[2]),rows)
0:plot_all_members(as.numeric(trends$k_value[3]),rows)
0:dev.off()
