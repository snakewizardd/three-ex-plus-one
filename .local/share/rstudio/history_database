0:# variables
0:x <- seq(xmin, xmax, length.out=nx)
0:y <- seq(ymin, ymax, length.out=ny)
0:c <- outer(x,y*1i,FUN="+")
0:z <- matrix(0.0, nrow=length(x), ncol=length(y))
0:k <- matrix(0.0, nrow=length(x), ncol=length(y))
0:for (rep in 1:n) {
0:index <- which(Mod(z) < 2)
0:z[index] <- .49*z[index]^2 + c[index]
0:k[index] <- k[index] + 1
0:}
0:if (showplot==TRUE) { image(x,y,k,col=cols, xlab="Re(c)", ylab="Im(c)")}
0:return(k)
0:}
